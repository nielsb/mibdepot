    ACCESS    not-accessible
    STATUS    optional --Conditionally Mandatory
    DESCRIPTION
        "
        The table for showing the general information of lpd. 
        "
    ::= { kmNetLpd 1 }
        
kmNetLpdGeneralEntry OBJECT-TYPE
    SYNTAX    KmNetLpdGeneralEntry
    ACCESS    not-accessible
    STATUS    optional --Conditionally Mandatory
    DESCRIPTION
        "
        "
    INDEX  { kmNetLpdIfIndex }
    ::= { kmNetLpdGeneralTable 1 }

   KmNetLpdGeneralEntry ::= SEQUENCE {
        kmNetLpdIfIndex    INTEGER,
        kmNetLpdEnable     INTEGER,
        kmNetLpdPort       INTEGER
        }

kmNetLpdIfIndex  OBJECT-TYPE
    SYNTAX     INTEGER
    ACCESS     read-only
    STATUS     optional --Conditionally Mandatory
    DESCRIPTION
        "
        The index for uniquely specifying NetworkInterface to 
        which lpd is set. 
        The value of index shall be the same value with 
        kmNetGeneralIndex (ifIndex).
        "
       ::= { kmNetLpdGeneralEntry 1 }

kmNetLpdEnable  OBJECT-TYPE
    SYNTAX  INTEGER {
            enable (1),
            disable (2)
            }  
    ACCESS  read-write
    STATUS  optional --Conditionally Mandatory
    DESCRIPTION
        "
