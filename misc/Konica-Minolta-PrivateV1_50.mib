    ACCESS    not-accessible
    STATUS    optional --Conditionally Mandatory
    DESCRIPTION
        "
        "
    INDEX  { kmNetNwIfIndex }
    ::= { kmNetNwGeneralTable 1 }

   KmNetNwGeneralEntry ::= SEQUENCE {
       kmNetNwIfIndex         INTEGER,
       kmNetNwDefault         INTEGER,
       kmNetNwFrameTypeConfig INTEGER,
       kmNetNwPrintMode       INTEGER
       }

kmNetNwIfIndex  OBJECT-TYPE
    SYNTAX     INTEGER
    ACCESS     read-only
    STATUS     optional --Conditionally Mandatory
    DESCRIPTION
        "The index for uniquely showing NetworkInterface.
         The value of index shall be the same value with 
         kmNetGeneralIndex (ifIndex).
        "
       ::= { kmNetNwGeneralEntry 1 }

kmNetNwDefault  OBJECT-TYPE
    SYNTAX     INTEGER{
                notSupported (0),
                supported (1),
                resetToDefault (2)
                }
    ACCESS     read-write
    STATUS     optional --Conditionally Mandatory
    DESCRIPTION
        "The command which restores all setup about NetWare to FactoryDefault. 

         When resetToDefault(2) is set, an agent side shall surely 
         return Reply.
         In principle, when resetToDefault(2) is set, about the timing
         in which the setup returns to default depends on an implementation
         and it does not define in MIB.
        "
       ::= { kmNetNwGeneralEntry 2 }

kmNetNwFrameTypeConfig  OBJECT-TYPE
    SYNTAX     INTEGER
    ACCESS     read-write
    STATUS     optional --Conditionally Mandatory
    DESCRIPTION
        "A setup of FrameType used by NetWare (IPX). 
         It is set by using Bitmap of the following.
         1 (Auto-Sense) is recommended for the Default.
         Although Auto-Sense and other value do not exist together,
         about Binding two or more FrameType simultaneously,
         it is dependent on the implementation of a model.

              Bit  Value      FrameType
              ---- ----       ----------
               0     1        AutoSense
               1     2        802.3
               2     4        EthernetII
               3     8        802.3_SNAP
               4    16        802.2 (4)
               5    32        Reserved
               6    64        802.5_TokenRing
               7   128        TokenRing_SNAP

        "
       ::= { kmNetNwGeneralEntry 3 }

kmNetNwPrintMode  OBJECT-TYPE
    SYNTAX     INTEGER{
                disable (0), -- IPX is working but Pint service is stopped.
                pServerMode (1),
                nPrinterMode (2)
                }
    ACCESS     read-write
    STATUS     optional --Conditionally Mandatory
    DESCRIPTION
        "The mode of NetworkInterface used by NetWare is set up.
        Default shall be pServerMode(1). When not supporting NPrinter,
         it shall be the implementation of RO.
        "
       ::= { kmNetNwGeneralEntry 4 }


