-- This Private MIB has the basic information which Network
-- Management Utility refers to.
-- The device that has network interface should implement 
-- this private MIB. 



IMPORTS

    DateAndTime                   FROM SNMPv2-TC
    enterprises, Integer32, Counter32, IpAddress
                                  FROM SNMPv2-SMI
    OBJECT-TYPE                   FROM RFC1212
    DisplayString, PhysAddress    FROM RFC1213-MIB
    hrDeviceIndex                 FROM HOST-RESOURCES-MIB
    Ipv6Address                   FROM IPV6-TC;


konicaminolta     OBJECT IDENTIFIER ::= { enterprises 18334 }
