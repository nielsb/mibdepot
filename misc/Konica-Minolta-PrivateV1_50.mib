-- Private Scanner MIB

kmScannerMib      OBJECT IDENTIFIER ::= { kmScanner 5 }

kmScnMibVersion  OBJECT-TYPE
    SYNTAX     DisplayString (SIZE(0..7))
    ACCESS     read-only
    STATUS     mandatory
    DESCRIPTION
        " 
         The version information of Konica-Minolta Standard MIB specification.

         This indicates the version of the Konica-Minolta Private Scanner MIB 
         which is implemented. 

         It is written as AA.BC.
         AA is indicating major revision, and B and C is 
         minor revision. Decimal point is ASCII-coded period.

         This definition file is corresponded to '1.50'.
        "
    ::= { kmScannerMib 1 }

   kmScnGeneral    OBJECT IDENTIFIER ::= { kmScannerMib 2 }
   kmScnODH        OBJECT IDENTIFIER ::= { kmScannerMib 3 }
   kmScnSensor     OBJECT IDENTIFIER ::= { kmScannerMib 4 }
   kmScnCapability OBJECT IDENTIFIER ::= { kmScannerMib 7 }
   kmScnAlert      OBJECT IDENTIFIER ::= { kmScannerMib 10 }


-- The General Scanner Group

-- This group shows general information of Scanner. 

kmScnGeneralTable OBJECT-TYPE
    SYNTAX     SEQUENCE OF KmScnGeneralEntry
    ACCESS     not-accessible
    STATUS     mandatory
    DESCRIPTION
         "
          A table of general information of scanner.
         "
    ::= { kmScnGeneral 1 }

kmScnGeneralEntry OBJECT-TYPE
    SYNTAX      KmScnGeneralEntry
    ACCESS      not-accessible
    STATUS      mandatory
    DESCRIPTION
        "
         An entry exists in this table for each
         device entry in the hostmib device table whose type
         is 'scanner'.
        "
     INDEX  {  hrDeviceIndex }
     ::= { kmScnGeneralTable 1 }

    KmScnGeneralEntry ::= SEQUENCE {
          kmScnGeneralReset             INTEGER,
          kmScnODHDefaultIndex          INTEGER,
          kmScnSensorDefaultIndex       INTEGER,
          kmScnGeneralStatus            INTEGER
         }

kmScnGeneralReset  OBJECT-TYPE
    SYNTAX    INTEGER{
                   notSupported (2),
                   notResetting (3),
                   powerCycleReset (4), -- Cold Start
                   resetToNVRAM (5), -- Warm Start
                   resetToFactoryDefaults (6) -- Reset contents of
                                              -- NVRAM to factory defaults
                   }
    ACCESS     read-write
    STATUS     mandatory
    DESCRIPTION
        "
         Setting this MIB will reset the scanner.
         Default value for this is noResetting(3).
         By setting resetToFactoryDefaults(6),
         all the setting should revert to the factory
         default settings.
         If the Scanner does not support 'Reset',
         device should return notSupported(2).
        "
    ::= { kmScnGeneralEntry 1 }

kmScnODHDefaultIndex OBJECT-TYPE
     SYNTAX     INTEGER(1..65535)
     ACCESS     read-write
     STATUS     mandatory
     DESCRIPTION
          "
          The value of kmScnODHIndex corresponding to the default ODH
          sub-unit.
          -- ODH (Original Document Handler)

          If device cannot support to change default ODH,
          this object may implement as ReadOnly.
          At least '1' must be implemented. 
          "
     ::= { kmScnGeneralEntry 2 }

kmScnSensorDefaultIndex OBJECT-TYPE
     SYNTAX     INTEGER(1..65535)
     ACCESS     read-write
     STATUS     mandatory
     DESCRIPTION
          "
          The value of kmScnSensorIndex corresponding to the default
          sensor sub-unit; that is, this object selects the default
          sensor.

          If device cannot support to change default Sensor,
          this object may implement as ReadOnly.
          At least '1' must be implemented. 
         "
     ::= { kmScnGeneralEntry 3 }

kmScnGeneralStatus OBJECT-TYPE
    SYNTAX    INTEGER {
                   unknown (0),
                   fatalError (1),
                   error (2),
                   warning (3),
                   scanning (4),
                   busy (5),
                   ready (6)
               }
    ACCESS     read-only
    STATUS     mandatory
    DESCRIPTION
         "Indicates scanner general status."
    ::= { kmScnGeneralEntry 4 }


kmScnCtrlName OBJECT-TYPE
    SYNTAX    OCTET STRING(SIZE(0..31))
    ACCESS     read-only
    STATUS     mandatory
    DESCRIPTION
         "
         This object shows the name of scanner controller.
         If device has no independent scanner controller or no 
         product name of it, this value should be  same as sysDescr.
         In case of printer, this value should be same as sysDescr.
        "  
    ::= { kmScnGeneral 2 }


--  The Original Document Handler Group

-- This group shows the information of Original Document Handler (ODH).

kmScnODHTable OBJECT-TYPE
     SYNTAX     SEQUENCE OF KmScnODHEntry
     ACCESS     not-accessible
     STATUS     mandatory
     DESCRIPTION
          "
           A table of the devices capable of providing original
          documents for input to the scanning process, handling those
          originals during scanning, and depositing the original
          documents in an output bin.
         "
     ::= { kmScnODH 1 }

kmScnODHEntry OBJECT-TYPE
     SYNTAX     KmScnODHEntry
     ACCESS     not-accessible
     STATUS     mandatory
     DESCRIPTION
          "Attributes of a Original Document Handler (ODH) device.
          Entries may exist in the table for each device index whose
          device type is `scanner'."
     INDEX  {  hrDeviceIndex, kmScnODHIndex }
     ::= { kmScnODHTable 1 }

     KmScnODHEntry ::= SEQUENCE {
          kmScnODHIndex                       INTEGER,
          kmScnODHType                        INTEGER,
          kmScnODHDimUnit                     INTEGER,
          kmScnODHMaxDimFeedDir               INTEGER,
          kmScnODHMaxDimXFeedDir              INTEGER,
          kmScnODHMinDimFeedDir               INTEGER,
          kmScnODHMinDimXFeedDir              INTEGER,
          kmScnODHCapacityUnit                INTEGER,
          kmScnODHInputMaxCapacity            INTEGER,
          kmScnODHInputCurrentLevel           INTEGER,
          kmScnODHOutputMaxCapacity           INTEGER,
          kmScnODHOutputRemainingCapacity     INTEGER,
          kmScnODHSpeedUnit                   INTEGER,
          kmScnODHMaxSpeed                    INTEGER,
          kmScnODHMaxMediaWeight              INTEGER,
          kmScnODHSimplexDuplex               INTEGER,
          kmScnODHDocumentScanOrder           INTEGER,
          kmScnODHInterleaving                INTEGER,
          kmScnODHDescription                 OCTET STRING
         }

kmScnODHIndex OBJECT-TYPE
     SYNTAX     INTEGER (1..65535)
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "A unique value used by the scanner to identify this ODH sub-
          unit. Although these values may change due to a major
          reconfiguration of the device (e.g. the addition of new ODH
          sub-units to the scanner), values are expected to remain
          stable across successive scanner power cycles."
     ::= { kmScnODHEntry 1 }

kmScnODHType OBJECT-TYPE
  --  This value is a type 2 enumeration
     SYNTAX     INTEGER {
          other(1),
          unknown(2),
          automaticDocumentFeeder(3),
          automaticBookReader(4),
          flatbedManual(5),
          sheetFeedManual(6),
          handScanner(7),
          digitalCamera(8)
         }
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The type of technology employed by the ODH sub-unit."
     ::= { kmScnODHEntry 2 }

kmScnODHDimUnit OBJECT-TYPE
     SYNTAX     INTEGER {
                other(1),
                unknown(2),
                tenThousandthsOfInches(3),  -- .0001
                micrometers(4)
                }
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The unit of measurement for use calculating and relaying
          dimensional values for this ODH sub-unit."
     ::= { kmScnODHEntry 3 }

kmScnODHMaxDimFeedDir OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "This object provides the value of the maximum dimension, in
          the feed direction, of the original document media that is
          accepted by this ODH sub-unit. The feed direction is parallel
          to the direction in which the media moves. This dimension is
          measured in ODH sub-unit dimensional units (kmScnODHDimUnit).
          This is NOT the size of each original. If the scanner is able
          to sense the size of the original during the scanning process
          of an individual original document, that information may be
          provided in the image data. The value (-1) means other and
          specifically means that this sub-unit places no restriction on
          this parameter. The value (-2) indicates unknown."
     ::= { kmScnODHEntry 4 }

kmScnODHMaxDimXFeedDir OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "This object provides the value of the maximum dimension, in
          the cross feed direction, of the media that is accepted by
          this input sub-unit. The cross  feed direction is ninety
          degrees relative to direction of media movement associated
          with this sub-unit. This dimension is measured in ODH sub-unit
          dimensional units (kmScnODHDimUnit). This is NOT the size of
          each original. If the scanner is able to sense the size of the
          original during the scanning process of an individual original
          document, that information may be provided in the image data.
          The value (-1) means other and specifically means that this
          sub-unit places no restriction on this parameter. The value (-
          2) indicates unknown."
     ::= { kmScnODHEntry 5 }

kmScnODHMinDimFeedDir OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "This object provides the value of the minimum dimension, in
          the feed direction, of the media that is accepted by this ODH
          sub-unit. This dimension is measured in ODH sub-unit
          dimensional units (kmScnODHDimUnit). This is NOT the size of
          each original. If the scanner is able to sense the size of the
          original during the scanning process of an individual original
          document, that information may be provided in the image data.
          The value (-1) means other and specifically means that this
          sub-unit places no restriction on this parameter. The value (-
          2) indicates unknown."
     ::= { kmScnODHEntry 6 }

kmScnODHMinDimXFeedDir OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "This object provides the value of the minimum dimension, in
          the cross feed direction, of the media that is accepted by
          this input sub-unit. The cross  feed direction is ninety
          degrees relative to the feed direction associated with this
          sub-unit. This dimension is measured in ODH sub-unit
          dimensional units (kmScnODHDimUnit). This is NOT the size of
          each original. If the scanner is able to sense the size of the
          original during the scanning process of an individual original
          document, that information may be provided in the image data.
          The value (-1) means other and specifically means that this
          sub-unit places no restriction on this parameter. The value (-
          2) indicates unknown."
     ::= { kmScnODHEntry 7 }

kmScnODHCapacityUnit OBJECT-TYPE
     SYNTAX     INTEGER{
                     other (1),
                     unknown (2),
                     tenThousandthsOfInches (3),  -- .0001
                     micrometers (4),
                     sheets (8),
                     feet (16),
                     meters (17)
                    }
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The unit of measurement for use in calculating and relaying
          capacity values for this input sub-unit."
     ::= { kmScnODHEntry 8 }

kmScnODHInputMaxCapacity OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The claimed maximum capacity of the input portion of the ODH
          sub-unit in ODH sub-unit capacity units (kmScnODHCapacityUnit).
          If this ODH sub-unit can reliably sense this value, the value
          is sensed by the scanner and may not be changed by management
          requests; otherwise, the value may be written (by a Remote
          Control Panel or a Management Application). The value (-1)
          means other and specifically indicates that the sub-unit
          places no restrictions on this parameter.  The value (-2)
          means unknown."
     ::= { kmScnODHEntry 9 }

kmScnODHInputCurrentLevel OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The current capacity of the input portion of the ODH sub-unit
          in ODH sub-unit capacity units (kmScnODHCapacityUnit). If this
          ODH sub-unit can reliably sense this value, the value is
          sensed by the scanner and may not be changed by management
          requests; otherwise, the value may be written (by a Remote
          Control Panel or a Management Application). The value (-1)
          means other and specifically indicates that the sub-unit
          places no restrictions on this parameter.  The value (-2)
          means unknown. The value (-3) means that the scanner knows
          that at least one unit remains."
     ::= { kmScnODHEntry 10 }

kmScnODHOutputMaxCapacity OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The claimed maximum capacity of the output portion of the ODH
          sub-unit in ODH sub-unit capacity units (kmScnODHCapacityUnit).
          If this ODH sub-unit can reliably sense this value, the value
          is sensed by the scanner and may not be changed by management
          requests; otherwise, the value may be written (by a Remote
          Control Panel or a Management Application). The value (-1)
          means other and specifically indicates that the sub-unit
          places no restrictions on this parameter.  The value (-2)
          means unknown."
     ::= { kmScnODHEntry 11 }

kmScnODHOutputRemainingCapacity OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The remaining capacity of output portion of the ODH sub-unit
          in ODH sub-unit capacity units (kmScnODHCapacityUnit). If this
          ODH sub-unit can reliably sense this value, the value is
          sensed by the scanner and may not be changed by management
          requests; otherwise, the value may be written (by a Remote
          Control Panel or a Management Application). The value (-1)
          means other and specifically indicates that the sub-unit
          places no restrictions on this parameter.  The value (-2)
          means unknown.  The value (-3) means that the scanner knows
          that there remains capacity for at least one unit."
     ::= { kmScnODHEntry 12 }

kmScnODHSpeedUnit OBJECT-TYPE
     SYNTAX     INTEGER{
                   other (1),
                   unknown (2),
                   tenThousandthsOfInchesPerHour(3),   -- .0001/hour
                   micrometersPerHour(4),
                   impressionsPerHour(7),
                   sheetsPerHour(8),
                   feetPerHour(16),
                   metersPerHour(17)
                  }
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The unit scanning speed of this ODH expressed in scans.
           If unit is impressionsPerHour(7) or sheetsPerHour(8),
           reference paper size is A4 or letter"
     ::= { kmScnODHEntry 13 }

kmScnODHMaxSpeed OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The maximum scanning speed of this ODH expressed in scans. 
           A value of (-1) implies 'other'.
           The value (-2) indicates unknown."
     ::= { kmScnODHEntry 14 }

kmScnODHMaxMediaWeight  OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The max weight of the medium associated with this input sub-
          unit in grams / per meter squared. The value (-1) means other
          and specifically indicates that the sub-unit places no
          restrictions on this parameter. The value (-2) means unknown."
     ::= { kmScnODHEntry 15 }

kmScnODHSimplexDuplex OBJECT-TYPE
  --   This value is a type 2 enumeration
     SYNTAX     INTEGER {
          other(1),
          unknown(2),
          simplex(3),
          duplexConcurrent(4),
          duplexSequential(5)
          }
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The type of scanning performed by this ODH and sensor
          combination. Simplex indicates that one side of the original
          document is sensed. DuplexConcurrent indicates that both sides
          of the document are imaged at the same time using dual
          sensors. DuplexSequential indicates that first one side and
          then the other side is imaged using the same sensor mechanism,
          with automatic flipping of the original document."
     ::= { kmScnODHEntry 16 }

kmScnODHDocumentScanOrder OBJECT-TYPE
  --   This value is a type 2 enumeration
     SYNTAX     INTEGER {
          other(1),
          unknown(2),
          firstToLast(3),
          lastToFirst(4)
          }
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The order of document traversal performed by this ODH.
          FirstToLast indicates that the first page of the document is
          scanned first and the last page is scanned last, which is
          'Scanner Order' or 'Fax Order'. LastToFirst indicates that the
          last page is scanned first and the first page is scanned last.
          This is 'Copier Order'."
     ::= { kmScnODHEntry 17 }

kmScnODHInterleaving OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The number of scans that the duplex pages are offset from
          each other. (0) indicates that the first side and opposite
          side is scanned concurrently. (1) indicates that the opposite
          side is scanned one page after the first side (i.e. 1,2,3. . .
          order). (2) indicates that the first side leads the opposite
          side by 2 pages (1,3,2,5,. . . order). (3) indicates that the
          first side leads the opposite side by 3 pages, and so forth.
          The value (32767) indicates that the entire first side is
          imaged prior to the opposite side. The terms 'first side' and
          'opposite side' is determined by the kmScnODHDocumentScanOrder
          attribute.

          The value (-1) means other and specifically indicates
          that the sub-unit places no restrictions on this parameter.
          The value (-2) means unknown."
     ::= { kmScnODHEntry 18 }

kmScnODHDescription OBJECT-TYPE
     SYNTAX     OCTET STRING (SIZE(0..255))
     ACCESS read-only
     STATUS     mandatory
     DESCRIPTION
          "The manufacturer-provided description of this ODH in the
          localization specified by prtGeneralCurrentLocalization.

          If no information for this sub-unit, this value should be 
          (zero_length)."
     ::= { kmScnODHEntry 19 }


-- The Image Sensor Group

kmScnSensorTable OBJECT-TYPE
     SYNTAX     SEQUENCE OF KmScnSensorEntry
     ACCESS     not-accessible
     STATUS     mandatory
     DESCRIPTION
          "This table contains information about the various image
          sensor sub-units in the scanner."
     ::= { kmScnSensor 1 }

kmScnSensorEntry OBJECT-TYPE
     SYNTAX     KmScnSensorEntry
     ACCESS     not-accessible
     STATUS     mandatory
     DESCRIPTION
          "Entries may exist in the table for each device index whose
          device type is `scanner'."
     INDEX  {  hrDeviceIndex, kmScnSensorIndex }
     ::= { kmScnSensorTable 1 }

     KmScnSensorEntry ::= SEQUENCE {
          kmScnSensorIndex                  INTEGER,
          kmScnSensorTech                   INTEGER,
          kmScnSensorLifeCount              Counter,
          kmScnSensorPowerOnCount           Counter,
          kmScnSensorColorSpace             INTEGER,
          kmScnSensorResolutionUnit         INTEGER,
          kmScnSensorOpticalResolFeedDir    INTEGER,
          kmScnSensorOpticalResolXFeedDir   INTEGER,
          kmScnSensorImageContrast          INTEGER,
          kmScnSensorImageBrightness        INTEGER,
          kmScnSensorXOffset                INTEGER,
          kmScnSensorYOffset                INTEGER,
          kmScnSensorOutputResolFeedDir     INTEGER,
          kmScnSensorOutputResolXFeedDir    INTEGER,
          kmScnSensorOpticalBitsPerPixel    INTEGER,
          kmScnSensorOutputBitsPerPixel     INTEGER
         }

kmScnSensorIndex OBJECT-TYPE
     SYNTAX     INTEGER (1..65535)
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "A unique value used by the scanner to identify this sensing
          sub-unit.  Although these values may change due to a major
          reconfiguration of the device (e.g. the addition of new
          marking sub-units to the scanner), values are expected to
          remain stable across successive scanner power cycles."
     ::= { kmScnSensorEntry 1 }

kmScnSensorTech OBJECT-TYPE
  -- This value is a type 2 enumeration
     SYNTAX     INTEGER {
          other(1),
          unknown(2),
          linearCCD(3),
          rectangularCCD(4),
          contactImageSensor(5),
          laserScanner(6)
         }
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The type of sensing technology used for this sensing sub-
          unit."
     ::= { kmScnSensorEntry 2 }

kmScnSensorLifeCount OBJECT-TYPE
     SYNTAX     Counter
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The count of the number of images scanned during the life of
          scanner.

          If device cannot support scan count function,
          this value should be always (0).
          "
     ::= { kmScnSensorEntry 3 }

kmScnSensorPowerOnCount OBJECT-TYPE
     SYNTAX     Counter
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The count of the number of images scanned since the equipment
          was most recently powered on.

          If device cannot support scan count function,
          this value should be always (0).
          "
     ::= { kmScnSensorEntry 4 }

kmScnSensorColorSpace OBJECT-TYPE
     SYNTAX     INTEGER {
                 other(1),
                 unknown(2),
                 colorSpaceNative(3),
                 colorSpacesRGB(4),           -- sRGB
                 colorSpaceCIEXYZ(5),         -- CIEXYZ
                 colorSpaceCIELAB(6),
                 colorSpaceCMYK(7),
                 colorSpaceBiLevelMinWhite(8), -- Each pixel is a single bit.
                                               --  0 is white.
                 colorSpaceBiLevelMinBlack(9), -- Each pixel is a single bit.
                                               --  0 is black.
                 colorSpaceGrayScale(10),      -- Each pixel is composed of a
                                               --  single gray sample.
                 colorSpacePallette(11)        -- Each pixel indexes into a
                 }                              --  color mapping table.
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The value of color space currently enabled for the
          sensor."
     ::= { kmScnSensorEntry 5 }

kmScnSensorResolutionUnit OBJECT-TYPE
  --   This value is a type 1 enumeration
     SYNTAX     INTEGER {
          other (1),
          unknown (2),
          tenThousandthsOfInches(3),  -- .0001
          micrometers(4)
          }
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The unit of measure of distances."
     ::= { kmScnSensorEntry 6 }

kmScnSensorOpticalResolFeedDir OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The number of addressable optical sensing positions in the
          feed direction per 10000 units of measure specified by
          Resolution Unit. A value of (-1) implies 'other' or 'infinite'
          while a value of (-2) implies 'unknown'."
     ::= { kmScnSensorEntry 7 }

kmScnSensorOpticalResolXFeedDir OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The number of addressable optical sensing positions in the
          cross feed direction in 10000 units of measure specified by
          Resolution Unit. A value of (-1) implies 'other' or 'infinite'
          while a value of (-2) implies 'unknown'."
     ::= { kmScnSensorEntry 8 }

kmScnSensorImageContrast OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "This value indicates the relative sharpness of the image,
          with 0 being the lowest possible and 100 the highest possible
          contrast.  The value of -1 means other and specifically
          indicates that the sensor is using an automatic adjustment
          feature.  For sensors where this setting is not continuously
          variable, a write to this object will set the value to the
          closest implemented value.  For example, a sensor with five
          possible settings can only achieve 0, 25, 50, 75, and 100. A
          write value of 30 will result in a read response of 25.

          The value (-2) means unknown.
          "
     ::= { kmScnSensorEntry 9 }

kmScnSensorImageBrightness OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "This value indicates the relative brightness of the image,
          with 0 being the lightest possible and 100 the darkest
          possible image.  The value of -1 means other and specifically
          indicates that the sensor is using an automatic adjustment
          feature.  For sensors where this setting is not continuously
          variable, a write to this object will set the value to the
          closest implemented value.  For example, a sensor with five
          possible settings can only achieve 0, 25, 50, 75, and 100. A
          write with a value of 30 will result in a read response of
          25.
          The value (-2) means unknown.
          "
     ::= { kmScnSensorEntry 10 }

kmScnSensorXOffset OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The offset, in units identified by Resolution Unit, from the X
          reference edge of the original document to the output scanned
          image.  The X reference edge is 180 degrees from the leading
          edge of the original document, as defined by the direction of
          the scan.  Negative margin values are typical and indicate
          that extra area is imaged that is never part of the original
          document.  Refer to the scan offsets figure.

          If device cannot recognize offset values,
          this value should be (0)."
     ::= { kmScnSensorEntry 11 }

kmScnSensorYOffset OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS read-only
     STATUS     mandatory
     DESCRIPTION
          "The offset, in units identified by Resolution Unit, from the
          leading edge of the original document, as defined by the
          direction of the scan, to the output scanned image.  Negative
          margin values are typical and indicate that extra area is
          imaged that is never part of the original document. Refer to
          the scan offsets figure.

          If device cannot recognize offset values,
          this value should be (0)."
     ::= { kmScnSensorEntry 12 }

kmScnSensorOutputResolFeedDir OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The number of addressable output image positions in the feed
          direction per 10000 units of measure specified by
          Resolution Unit. A value of (-1) implies 'other' or 'infinite'
          while a value of (-2) implies 'unknown'."
     ::= { kmScnSensorEntry 13 }

kmScnSensorOutputResolXFeedDir OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The number of addressable output image positions in the cross
          feed direction in 10000 units of measure specified by
          Resolution Unit. A value of (-1) implies 'other' or 'infinite'
          while a value of (-2) implies 'unknown'."
     ::= { kmScnSensorEntry 14 }

kmScnSensorOpticalBitsPerPixel OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The number of bits used to describe a single pixel, as
          generated by the sensor in a single frequency range sample."
     ::= { kmScnSensorEntry 15 }


kmScnSensorOutputBitsPerPixel OBJECT-TYPE
     SYNTAX     INTEGER
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "The number of bits used to describe a single pixel, as output
          by the scanner for a single frequency range sample."
     ::= { kmScnSensorEntry 16 }


-- Scanner Capability Group


kmScnCapabilityTable OBJECT-TYPE
     SYNTAX     SEQUENCE OF KmScnCapabilityEntry
     ACCESS     not-accessible
     STATUS     mandatory
     DESCRIPTION
          "
           This table shows the capability of scanner function.
          "
     ::= { kmScnCapability 1 }

kmScnCapabilityEntry OBJECT-TYPE
     SYNTAX     KmScnCapabilityEntry
     ACCESS     not-accessible
     STATUS     mandatory
     DESCRIPTION
          "
           The information of scanner capability. 
          " 
     INDEX  {  hrDeviceIndex, kmScnCapabilityIndex }
     ::= { kmScnCapabilityTable 1 }

    KmScnCapabilityEntry ::= SEQUENCE {
            kmScnCapabilityIndex    INTEGER,
            kmScnCapSupportMask     INTEGER,
            kmScnCapResolution      INTEGER,
            kmScnCapColorMode       INTEGER,
            kmScnCapCompressionForm INTEGER,
            kmScnCapFileFormat      INTEGER,
            kmScnCapProtocol        INTEGER,
            kmScnCapScalingMin      INTEGER,
            kmScnCapScalingMax      INTEGER,
            kmScnCapBitDepth        INTEGER,
            kmScnCapDensityMin      INTEGER,
            kmScnCapDensityMax      INTEGER,
            kmScnCapImageEnhance    INTEGER,
            kmScnCapSpecialOriginal INTEGER,
            kmScnCapBookCopyMode    INTEGER,
            kmScnCapImageFeature    INTEGER,
            kmScnCapEraseMin        INTEGER,
            kmScnCapEraseMax        INTEGER,
            kmScnCapFoldEraseMin    INTEGER,
            kmScnCapFoldEraseMax    INTEGER
         }

kmScnCapabilityIndex OBJECT-TYPE
     SYNTAX     INTEGER (1..65535)
     ACCESS     read-only
     STATUS     mandatory
     DESCRIPTION
          "
          Index for scanner capability.
          Normally, this value should be 1.
          "
     ::= { kmScnCapabilityEntry 1 }

kmScnCapSupportMask  OBJECT-TYPE
    SYNTAX    INTEGER
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
        "
         Supported function of scanner is expressed
         with Bitmap.

           Bit  Value    Function
           ---   -----  ---------------
            0       1   scaling -- Zoom
            1       2   density
            2       4   duplex  -- Double sides
            3       8   rotate
            4      16   originalDirection
            5      32   splitDocument
        "
    ::= { kmScnCapabilityEntry 2 }

kmScnCapResolution  OBJECT-TYPE
    SYNTAX    INTEGER
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
        "
         Supported resolution of scanner is expressed
         with Bitmap.

           Bit  Value   Resolution
           ---   -----  ------------
            0       1   200dpi
            1       2   300dpi
            2       4   400dpi
            3       8   600dpi
            4      16   200x100dpi

        "
    ::= { kmScnCapabilityEntry 3 }

kmScnCapColorMode  OBJECT-TYPE
    SYNTAX    INTEGER
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
        "
         Supported color mode of scanner is expressed
         with Bitmap.

           Bit  Value   Resolution
           ---   -----  ------------
            0       1   black
            1       2   grayscale
            2       4   color
            3       8   Auto

        "
    ::= { kmScnCapabilityEntry 4 }


kmScnCapCompressionForm  OBJECT-TYPE
    SYNTAX    INTEGER
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
        "
         Supported compression form of scanner is expressed
         with Bitmap.

           Bit  Value   Compression Form
           ---   -----  ------------
            0       1   MH
            1       2   MR
            2       4   MMR
            3       8   JPEG
            4      16   JPEG-HighQuality
            5      32   JPEG-Standard
            6      64   JPEG-LowQuality

        "
    ::= { kmScnCapabilityEntry 5 }

kmScnCapFileFormat  OBJECT-TYPE
    SYNTAX    INTEGER
