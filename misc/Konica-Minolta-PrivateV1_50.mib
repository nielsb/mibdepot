    SYNTAX      KmPrtPaperSizeEntry
    ACCESS      not-accessible
        STATUS      mandatory
        DESCRIPTION
        "
        An entry exists in this table for each
        device entry in the hostmib device table whose type
        is 'printer'
        "
        INDEX  { hrDeviceIndex, kmPrtPaperSizeIndex }
        ::= {kmPrtPaperSizeTable 1 }

    KmPrtPaperSizeEntry ::= SEQUENCE {
        kmPrtPaperSizeIndex   INTEGER,
        kmPrtPaperSizeName    OCTET STRING,
        kmPrtPaperFeedDir     INTEGER
        }

kmPrtPaperSizeIndex  OBJECT-TYPE
    SYNTAX   INTEGER (1..65535) 
    ACCESS   read-only
    STATUS   mandatory
    DESCRIPTION
        "
        A unique value used to identify this entry. 
        "
      ::= { kmPrtPaperSizeEntry  1 }

kmPrtPaperSizeName  OBJECT-TYPE
    SYNTAX   OCTET STRING (SIZE(0..31)) 
