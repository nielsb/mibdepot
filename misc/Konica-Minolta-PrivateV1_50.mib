-- Scanner Alert Group
-- This group shows alert message of scanner.
-- If the device has the printer engine(include FAX),
-- the kmScnAlertGroup's alert must be included in the prtAlert Group
-- of Printer MIB.(The device must not have the kmScnAlertGroup). 
-- If the device does not have the printer engine,
-- the kmScnAlertGroup's alert must be as it is
-- (Because the device does not have the PrinterMIB.).

kmScnAlertTable OBJECT-TYPE
    SYNTAX     SEQUENCE OF KmScnAlertEntry
    ACCESS     not-accessible
    STATUS     optional
    DESCRIPTION
        "
        "
    ::= { kmScnAlert 1 }

kmScnAlertEntry OBJECT-TYPE
    SYNTAX     KmScnAlertEntry
    ACCESS     not-accessible
    STATUS     optional
    DESCRIPTION
        "
        Entries may exist in the table for each device
        index with a device type of 'scanner'.
        "
    INDEX  { hrDeviceIndex, kmScnAlertIndex }
    ::= { kmScnAlertTable 1 }

KmScnAlertEntry ::= SEQUENCE {
    kmScnAlertIndex               INTEGER,
    kmScnAlertSeverityLevel       INTEGER,
    kmScnAlertTrainingLevel       INTEGER,
    kmScnAlertGroup               INTEGER,
    kmScnAlertLocation            INTEGER,
    kmScnAlertDescription         OCTET STRING,
    kmScnAlertTime                TimeTicks
    }

kmScnAlertIndex OBJECT-TYPE
    SYNTAX     INTEGER (1..65535)
    ACCESS      read-only
    STATUS     optional
    DESCRIPTION
         "
         The index value used to determine which alerts have
         been added or removed from the alert table. This is an
         incrementing INTEGER starting from zero every time the
         scanner is reset.  When the scanner adds an alert to the
         table, that alert is assigned the next higher INTEGER
         value from the last item entered into the table.  If the
         index value reaches its maximum value, the next item
         entered will cause the index value to roll over and
         start at zero again.  The first event placed in the
         alert table after a reset of the printer shall have an
         index value of 1. 
         "
    ::= { kmScnAlertEntry 1 }

kmScnAlertSeverityLevel OBJECT-TYPE
    -- This value is a type 1 enumeration
    SYNTAX    INTEGER {
                   other(1),
                   criticalBinaryChangeEvent(3),
                   warningUnaryChangeEvent(4),
                   warningBinaryChangeEvent(5)
               }
    ACCESS read-only
    STATUS     optional
    DESCRIPTION
         "
         The level of severity of this alert table entry.  The
         scanner determines the severity level assigned to each
         entry into the table.
         "
    ::= { kmScnAlertEntry 2 }

kmScnAlertTrainingLevel OBJECT-TYPE
    -- This value is a type 2 enumeration
    SYNTAX     INTEGER {
                   other(1),
                   unknown(2),
                   untrained(3),
                   trained(4),
                   fieldService(5),
                   management(6),
                   noInterventionRequired(7)
               }
    ACCESS read-only
    STATUS     optional
    DESCRIPTION
         "
         The level of training of this alert table entry.  The
         scanner determines the training level assigned to each
         entry into the table.
         "
    ::= { kmScnAlertEntry 3 }

kmScnAlertGroup OBJECT-TYPE
    -- This value is a type 1 enumeration
    SYNTAX     INTEGER {
                      other(1),
                      hostResourcesMIBStorageTable(3),
                      hostResourcesMIBDeviceTable(4),
                   -- Values of Konica-Minolta Private Scanner MIB
                      generalScanner (51),
                      originalDocumentHnadler (52),
                      sensor (53)
                     }
    ACCESS read-only
    STATUS     optional
    DESCRIPTION
         "
         The type of sub-unit within the scanner model that this
         alert is related. 
         "
    ::= { kmScnAlertEntry 4 }

kmScnAlertLocation OBJECT-TYPE
    SYNTAX     INTEGER
    ACCESS     read-only
    STATUS     optional
    DESCRIPTION
         "
         The codes to further refine the alert information.
         "
    ::= { kmScnAlertEntry 5 }

kmScnAlertDescription OBJECT-TYPE
    SYNTAX     OCTET STRING (SIZE(0..255))
    ACCESS     read-only
    STATUS     optional
    DESCRIPTION
         "
         A description of this alert entry in the localization
         specified by prtGeneralCurrentLocalization.
         "
    ::= { kmScnAlertEntry 6 }

kmScnAlertTime OBJECT-TYPE
    SYNTAX     TimeTicks
    ACCESS     read-only
    STATUS     optional
    DESCRIPTION
         "
         The value of sysUpTime at the time that this alert was
         generated.

         If device cannot detect the time of error occurred,
         this value should be (0).
         "
    ::= { kmScnAlertEntry 7 }


-- Private Network MIB

    kmNetworkMib     OBJECT IDENTIFIER ::= { kmNetwork 5 }


    kmNetMibVersion  OBJECT-TYPE
    SYNTAX     DisplayString (SIZE(0..7))
    ACCESS     read-only
    STATUS     mandatory
    DESCRIPTION
        "
         The version information of Konica-Minolta Standard MIB specification.

         This indicates the version of the Konica-Minolta Private Network MIB 
         which is implemented. 

         It is written as AA.BC.
         AA is indicating major revision, and B and C is 
         minor revision. Decimal point is ASCII-coded period.

         This definition file is corresponded to '1.50'.
        "
    ::= { kmNetworkMib 1 }


    kmNetGeneral       OBJECT IDENTIFIER ::= { kmNetworkMib 2 }
    kmNetProtocol      OBJECT IDENTIFIER ::= { kmNetworkMib 3 }

    kmNetCommand       OBJECT IDENTIFIER ::= { kmNetworkMib 5 }
    kmNetSnmp          OBJECT IDENTIFIER ::= { kmNetworkMib 6 }
    kmNetTcpip         OBJECT IDENTIFIER ::= { kmNetworkMib 7 }
    kmNetNetWare       OBJECT IDENTIFIER ::= { kmNetworkMib 8 }
    kmNetAppleTalk     OBJECT IDENTIFIER ::= { kmNetworkMib 9 }
    kmNetSmb           OBJECT IDENTIFIER ::= { kmNetworkMib 10 }

-- kmNetGeneral Group

-- The general information that Network has is included.
-- It shall have the followings as general information.
-- 1.   Version information of NetworkInterface
-- 2.   Hardware address
-- 3.   Cable related information


kmNetGeneralTable  OBJECT-TYPE
    SYNTAX    SEQUENCE OF KmNetGeneralEntry
    ACCESS    not-accessible
    STATUS    mandatory
    DESCRIPTION
        "
        Table for showing the general information of
        Network Interface. 
        "
    ::= { kmNetGeneral 1 }

kmNetGeneralEntry OBJECT-TYPE
    SYNTAX    KmNetGeneralEntry
    ACCESS    not-accessible
    STATUS    mandatory
    DESCRIPTION
        "
        General information of network interface.
        "
    INDEX  { kmNetGeneralIndex }
    ::= { kmNetGeneralTable 1 }

    KmNetGeneralEntry ::= SEQUENCE {
        kmNetGeneralIndex       INTEGER,
        kmNetFirmVersion        OCTET STRING,
        kmNetHardwareAddress    PhysAddress,
        kmNetSerialNumber       OCTET STRING,
        kmNetSpeedConfig        INTEGER,
        kmNetTimeout            INTEGER
        }

kmNetGeneralIndex  OBJECT-TYPE
    SYNTAX     INTEGER
    ACCESS     read-only
    STATUS     mandatory
    DESCRIPTION
           "
            Index for showing NetworkInterface installed to a device. 
            Basically, it shall take the same value with ifIndex. 
           "
       ::= { kmNetGeneralEntry 1 }

kmNetFirmVersion  OBJECT-TYPE
    SYNTAX    OCTET STRING(SIZE(0..7))
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
        "
         Shows the FW version of NetworkInterface. 
         Write it as ##.xx.
         ## shall be a major version and xx shall
         be a minor version.
         If there is no original FW of NetworkInterface, 
         implement character strings of '-' (hyphen).
        "
    ::= { kmNetGeneralEntry 2 }

kmNetHardwareAddress  OBJECT-TYPE
    SYNTAX    PhysAddress
    ACCESS    read-write
    STATUS    mandatory
    DESCRIPTION
        "
         MAC address of NetworkInterface. (Hardware address)
         In the device of our company, it was made into RW since 
         MAC address needs to be written at the time of shipment.
         Implement as RO when the writing of MAC address is unnecessary.
         Moreover, in a management tool side, be sure to handle it as RO.
        "
    ::= { kmNetGeneralEntry 3 }

kmNetSerialNumber  OBJECT-TYPE
    SYNTAX    OCTET STRING(SIZE(0..15))
    ACCESS    read-write
    STATUS    mandatory
    DESCRIPTION
        "
         The serial number of NetworkInterface.
         Implement '-' (hyphen) in the device without a serial number.
         In the device of our company, it was made into RW since serial 
         number may need to be written at the time of shipment.
         Implement as RO when the writing of serial number is unnecessary.
         Moreover, in a management tool side, be sure to handle it as RO.
        "
    ::= { kmNetGeneralEntry 4 }

kmNetSpeedConfig  OBJECT-TYPE
    SYNTAX    INTEGER {
                 other (0),
                 autoDetectEther (1),
                 et10MbpsHalfDuplex (2),
                 et10MbpsFullDuplex (3),
                 et100MbpsHalfDuplex (4),
                 et100MbpsFullDuplex (5),
                 etGbpsHalfDuplex (6),
                 etGbpsFullDuplex (7),

                 autoDetectToken (10),
                 tr4Mbps (11),
                 tr16Mbps (12),
                 tr32Mbps (13),

                 autoDetectWireless (20),
                 wireless1Mbps (21),
                 wireless2Mbps (22),
                 wireless5-5Mbps (23),
                 wireless11Mbps (24),
                 wireless54Mbps (25)
              }
    ACCESS    read-write
    STATUS    mandatory
    DESCRIPTION
        "
         Sets up the speed of NetworkInterface. 
         Refer to ifSpeed for checking the present speed of operation.

         If this setting can not be set from Network access, a device
         should implement a current set value with RO access.
        "
    ::= { kmNetGeneralEntry 7 }

kmNetTimeout  OBJECT-TYPE
    SYNTAX    INTEGER
    ACCESS    read-write
    STATUS    optional
    DESCRIPTION
        "
        The setting of network interface time out.
        Setting range depends device specification.
        Unit is seconds.

        In unified specification, range is 10 to 300, 
        default value is 15.

        If a device cannot support this function,
        This value must be -1 as RO.
        "
    ::= { kmNetGeneralEntry 8 }


-- kmNetProtocol Group

-- The information of Network Protocol that 
-- NetworkInterface has is included.

kmNetProtocolTable  OBJECT-TYPE
    SYNTAX    SEQUENCE OF KmNetProtocolEntry
    ACCESS    not-accessible
    STATUS    mandatory
    DESCRIPTION
        "
         The table for showing the protocol information 
         which NetworkInterface supports. 
        "
    ::= { kmNetProtocol 1 }

kmNetProtocolEntry OBJECT-TYPE
    SYNTAX    KmNetProtocolEntry
    ACCESS    not-accessible
    STATUS    mandatory
    DESCRIPTION
        "
         The information of protocol.
        "
    INDEX  { kmNetProtocolIfIndex, kmNetProtocolIndex }
    ::= { kmNetProtocolTable 1 }

    KmNetProtocolEntry ::= SEQUENCE {
        kmNetProtocolIfIndex   INTEGER,
        kmNetProtocolIndex     INTEGER,
        kmNetProtocolType      INTEGER,
        kmNetProtocolDescr     OCTET STRING,
        kmNetProtocolVer       OCTET STRING,
        kmNetProtocolOnOff     INTEGER
        }

kmNetProtocolIfIndex  OBJECT-TYPE
    SYNTAX     INTEGER
    ACCESS     read-only
    STATUS     mandatory
    DESCRIPTION
           "
            Index for showing NetworkInterface that a system has. 
            The value of index shall be the same value with 
            kmNetGeneralIndex (ifIndex).
           "
       ::= { kmNetProtocolEntry 1 }

kmNetProtocolIndex  OBJECT-TYPE
    SYNTAX     INTEGER
    ACCESS     read-only
    STATUS     mandatory
    DESCRIPTION
           "
            Index for uniquely showing the protocol which
            a system supports. Index shall be assigned in 
            the following order is recommended.

            No. Protocol
            --------------------
            1   TCP/IP
            2   NetWare(IPX/SPX)
            3   AppleTalk
            4   SMB

           "
       ::= { kmNetProtocolEntry 2 }

kmNetProtocolType  OBJECT-TYPE
    SYNTAX    INTEGER {
                unknown (0),
                tcp-ip  (1),
                netWare (2),
                appleTalk (3),
                smb     (4),
                other   (7)
              }
    ACCESS    read-only
    STATUS    mandatory
    DESCRIPTION
        "
         Shows the type of supporting protocol. 
        "
    ::= { kmNetProtocolEntry 3 }

kmNetProtocolDescr  OBJECT-TYPE
    SYNTAX   OCTET STRING (SIZE(0..15))
    ACCESS   read-only
    STATUS     mandatory
    DESCRIPTION
        "
         Description that shows a protocol. 
         Although the value to indicate shall depend on 
         an implementation, the general protocol name, 
         such as 'TCP/IP', 'NetWare', etc., shall be indicated 
         in principle. Implementation of Null is not accepted.
        "
    ::= { kmNetProtocolEntry 4 }

kmNetProtocolVer  OBJECT-TYPE
    SYNTAX  OCTET STRING (SIZE(0..15))
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "
         The version of the implemented protocol stack. 
         Implement '-' (hyphen) when there is no version notation. 
         Implementation of Null is not accepted.
        "
    ::= { kmNetProtocolEntry 5 }

kmNetProtocolOnOff  OBJECT-TYPE
    SYNTAX  INTEGER {
            notSupported (0),
            enable (1),
            disable (2)
            }  
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
        "
         Sets up a states display of On and Off of 
         the function of each protocol. 
         If this setting can not be set from Network access, 
         a device should implement a current set value with RO access.
        "
    ::= { kmNetProtocolEntry 6 }


-- kmNetCommand Group

-- The various command information which 
-- NetworkInterface has is included.

kmNetCommandTable  OBJECT-TYPE
    SYNTAX    SEQUENCE OF KmNetCommandEntry
    ACCESS    not-accessible
    STATUS    mandatory
    DESCRIPTION
        "
         The table for showing the command information 
         which NetworkInterface supports. 
        "
    ::= { kmNetCommand 1 }

kmNetCommandEntry OBJECT-TYPE
    SYNTAX    KmNetCommandEntry
    ACCESS    not-accessible
    STATUS    mandatory
    DESCRIPTION
        "
         The information of command for network interface.
        "
    INDEX  { kmNetCommandIfIndex }
    ::= { kmNetCommandTable  1 }

    KmNetCommandEntry ::= SEQUENCE {
        kmNetCommandIfIndex        INTEGER,
        kmNetCommandReset          INTEGER,
        kmNetCommandDefault        INTEGER
        }

kmNetCommandIfIndex  OBJECT-TYPE
    SYNTAX     INTEGER
    ACCESS     read-only
    STATUS     mandatory
    DESCRIPTION
        "
         Index for uniquely showing the destination of command. 
         The value of index shall be the same value with 
         kmNetGeneralIndex (ifIndex).
        "
    ::= { kmNetCommandEntry 1 }

kmNetCommandReset  OBJECT-TYPE
    SYNTAX    INTEGER {
                notSupported (0),
                supported (1),
                reset (2)
               }
    ACCESS    read-write
    STATUS    mandatory
    DESCRIPTION
        "
         The command that performs independent reset of 
         NetworkInterface. 

         When reset(2) is set, an agent side shall surely 
         return Reply. Implementation that performs Reset 
         immediately without returning Reply is not accepted. 
         Return Reply of an error, when Reset command is invalid.
         After reset(2) is set, about the timing which is 
         actually performing Reset depends on an implementation, 
         and it shall not be defined in MIB.
        "
    ::= { kmNetCommandEntry 2 }

kmNetCommandDefault  OBJECT-TYPE
    SYNTAX    INTEGER {
                notSupported (0),
                supported (1),
                resetToDefault (2)
              }
    ACCESS    read-write
    STATUS    mandatory
    DESCRIPTION
        "
         The command for returning a setup about 
         NetworkInterface to FactoryDefault. 

         When resetToDefault(2) is set, an agent side 
         shall surely return Reply.
         In principle, when resetToDefault(2) is set, 
         about the timing in which the setup returns to 
         default depends on an implementation and it does 
         not define in MIB.
        "
    ::= { kmNetCommandEntry 3 }

--  kmNetSnmp Group

-- SNMP related information that NetworkInterface has is included.

kmNetSnmpCommTable  OBJECT-TYPE
    SYNTAX    SEQUENCE OF KmNetSnmpCommEntry
    ACCESS    not-accessible
    STATUS    mandatory
    DESCRIPTION
        "
         The table for showing the information of Community 
         of SNMP which a system has. 
        
         In addition, Community name shall be the common to 
         the whole system on the premise, and shall not have 
         independent Community for every NetworkInterface.
         In having Community independently by Intelligent NIC etc., 
         it shall have NetworkMIB independently to each and is not 
         managed by unified agent.
        "
    ::= { kmNetSnmp 1 }

kmNetSnmpCommEntry OBJECT-TYPE
    SYNTAX    KmNetSnmpCommEntry
    ACCESS    not-accessible
    STATUS    mandatory
    DESCRIPTION
        "
        "
    INDEX  { kmNetSnmpCommIndex }
    ::= { kmNetSnmpCommTable 1 }

    KmNetSnmpCommEntry ::= SEQUENCE {
        kmNetSnmpCommIndex         INTEGER,
        kmNetSnmpCommName          DisplayString,
        kmNetSnmpCommAccessRight   INTEGER
    }

kmNetSnmpCommIndex  OBJECT-TYPE
    SYNTAX     INTEGER
    ACCESS     read-only
    STATUS     mandatory
    DESCRIPTION
       "
        Index for identifying each SNMP community.
        Although about Community number to support, 
        Community name, and each access rights are 
        dependent on an implementation.
        In default, following three Communities is recommended.
        
        public (RO)
        private (RW)
        admin (RW)

        It shall be indispensable at least to have 
        public (RW) or to have the two of public (RO) 
        and private (RW).
       "
       ::= { kmNetSnmpCommEntry 1 }

kmNetSnmpCommName  OBJECT-TYPE
    SYNTAX    DisplayString(SIZE(1..15))
    ACCESS    read-write
    STATUS    mandatory
    DESCRIPTION
        "
         Community name of SNMP. Having the above-mentioned 
         name as default is recommended. This item does not 
         accept the implementation of RO. Community name has 
         to be changeable.

         But if Community name can not be changeable, 
         'public' or 'private' is recommended to be implemented 
         as Community name with RO access.
        "
    ::= { kmNetSnmpCommEntry 2 }

kmNetSnmpCommAccessRight  OBJECT-TYPE
    SYNTAX    INTEGER {
                none (0),
                readOnly (1),
                writeOnly (2),
                readWrite (3)
              }
    ACCESS    read-write
    STATUS    mandatory
    DESCRIPTION
        "
        Shows the access rights of SNMP community. 
        If this setting can not be set from Network access, 
        a device should implement a current set value with RO access.
        "
    ::= { kmNetSnmpCommEntry 3 }


